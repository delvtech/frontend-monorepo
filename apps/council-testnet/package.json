    "@alch/alchemy-web3": "^1.1.7",
    "@elementfi/council-typechain": "*",
    "@elementfi/council-tokenlist": "*",
    "@ethersproject/providers": "^5.6.5",
    "@uniswap/token-lists": "^1.0.0-beta.26",
    "dotenv": "^16.0.0",
    "ethernal": "^0.8.0",
    "hardhat-ethernal": "^0.3.10",
    "merkletreejs": "^0.2.24",
    "tsc": "^2.0.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-vyper": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/ethereumjs-abi": "^0.6.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.20",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethers": "^5.6.6",
    "hardhat": "^2.6.6",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typechain": "^5.2.0",
    "typechain-target-ethers-v5": "^5.0.1",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "npx hardhat node",
    "start-ethernal": "cross-env SYNC_ETHERNAL=1 npx hardhat node",
    "build-tokenlist": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat build-tokenlist --network localhost",
    "deploy-contracts": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/main.ts --network localhost --no-compile && npm run build-tokenlist",
    "test-claiming": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/testClaiming.ts --network localhost --no-compile",
    "test-proposal": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/createProposal.ts --network localhost --no-compile",
    "create-goerli-gsc-proposal": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat createGoerliGscProposal --network goerli",
    "create-proposals": "bash scripts/create-proposals.sh",
    "create-gsc-proposals": "bash scripts/create-gsc-proposals.sh",
    "mine-auto": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat autoMine",
    "mine-interval": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat intervalMining",
    "ethernal-listen": "npx ethernal listen -l",
    "ethernal-reset": "npx ethernal \"Hardhat Network\""
  }
}
