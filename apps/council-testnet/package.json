{
  "name": "council-testnet",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/element-fi/elf-council-testnet",
  "description": "",
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.7",
    "@elementfi/council-typechain": "*",
    "@elementfi/council-tokenlist": "*",
    "@elementfi/eslint-config": "*",
    "@elementfi/prettier-config": "*",
    "@ethersproject/providers": "^5.6.5",
    "@uniswap/token-lists": "^1.0.0-beta.26",
    "dotenv": "^16.0.0",
    "ethernal": "^0.14.0",
    "hardhat-ethernal": "^0.3.10",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "merkletreejs": "^0.2.24",
    "tsc": "^2.0.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-vyper": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/ethereumjs-abi": "^0.6.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.20",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "ethers": "^5.6.6",
    "hardhat": "^2.6.6",
    "prettier": "2.6.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.11.0",
    "typechain": "^5.2.0",
    "typechain-target-ethers-v5": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "npx hardhat node",
    "start-ethernal": "cross-env SYNC_ETHERNAL=1 npx hardhat node",
    "build-tokenlist": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat build-tokenlist --network localhost",
    "deploy-contracts": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/main.ts --network localhost --no-compile && npm run build-tokenlist",
    "test-claiming": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/testClaiming.ts --network localhost --no-compile",
    "test-proposal": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat run src/scripts/createProposal.ts --network localhost --no-compile",
    "create-goerli-gsc-proposal": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat createGoerliGscProposal --network goerli",
    "create-proposals": "bash scripts/create-proposals.sh",
    "create-gsc-proposals": "bash scripts/create-gsc-proposals.sh",
    "mine-auto": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat autoMine",
    "mine-interval": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat intervalMining",
    "ethernal-listen": "npx ethernal listen -l",
    "ethernal-reset": "npx ethernal \"Hardhat Network\"",
    "prepare": "husky install",
    "lint:w": "eslint --fix '**/*.{gql,graphql,js,jsx,ts,tsx,json,md}'",
    "lint": "eslint '**/*.{gql,graphql,js,jsx,ts,tsx,json,md}'",
    "prettier:w": "prettier --write '**/*.{gql,graphql,js,jsx,ts,tsx,json,md}'",
    "prettier:check": "prettier --check '**/*.{gql,graphql,js,jsx,ts,tsx,json,md}'"
  },
  "lint-staged": {
    "**/*.{gql,graphql,js,jsx,ts,tsx,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
