{
  "name": "@elementfi/core-v2-sdk",
  "version": "0.1.0",
  "description": "Element SDK for V2 contracts",
  "license": "Apache-2.0",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "targets": {
    "main": {
      "includeNodeModules": []
    }
  },
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "alias": {
    "src": "./src"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 npx hardhat test",
    "example": "ts-node -r dotenv/config -r tsconfig-paths/register src/example",
    "watch:example": "nodemon -r dotenv/config -r tsconfig-paths/register src/example/index.ts",
    "prepare": "husky install",
    "lint:w": "eslint --fix **/*.{gql,graphql,js,jsx,ts,tsx,json,md}",
    "lint": "eslint **/*.{gql,graphql,js,jsx,ts,tsx,json,md}",
    "prettier:w": "prettier --write **/*.{gql,graphql,js,jsx,ts,tsx,json,md}",
    "prettier:check": "prettier --check **/*.{gql,graphql,js,jsx,ts,tsx,json,md}"
  },
  "lint-staged": {
    "**/*.{gql,graphql,js,jsx,ts,tsx,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/element-fi/frontend-monorepo.git"
  },
  "author": "Element Finance (https://www.element.fi)",
  "bugs": {
    "url": "https://github.com/element-fi/frontend-monorepo/issues"
  },
  "homepage": "https://github.com/element-fi/frontend-monorepo/tree/main/packages/core-v2-sdk#readme",
  "dependencies": {
    "@elementfi/base": "*",
    "@elementfi/core-v2-typechain": "*",
    "lru-cache": "^7.14.0"
  },
  "devDependencies": {
    "@elementfi/eslint-config": "*",
    "@elementfi/prettier-config": "*",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.20.52",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "7.32.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "parcel": "^2.6.0",
    "prettier": "2.6.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typechain": "^5.2.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "ethers": ">=5.6"
  }
}
